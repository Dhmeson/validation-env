{
  "name": "validation-env",
  "version": "2.0.1",
  "description": "Manages the environment variables necessary for the application. Loads environment  variables from the .env file and checks that all required variables are defined.",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js"
    }
  },
  "files": [
    "dist",
    "readme.md"
  ],
  "sideEffects": false,
  "icon": "https://res.cloudinary.com/dbhcx7cfh/image/upload/v1760720853/logo.730Z_umnuze.png",
  "repository": {
    "url": "https://github.com/Dhmeson/validation-env"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "env-cli",
    "environment",
    "validation-env",
    "env-manager",
    "env"
  ],
  "author": "Dhmeson Noronha Araujo",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true

  },
  "devDependencies": {
    "tsx": "^4.16.2",
    "typescript": "^5.9.3",
    "@types/node": "^20.16.13"

  },
  "dependencies": {
    "dotenv": "^17.2.3"
  }

}
